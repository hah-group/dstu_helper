{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitReturns": false,
    "paths": {
      "@dstu_helper/common": [
        "libs/common/src"
      ],
      "@dstu_helper/bot": [
        "libs/bot/src"
      ]
    }
  }
}
