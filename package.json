{
  "name": "dstu-helper",
  "version": "2.0.3",
  "description": "DSTU Helper bot for VK and Telegram",
  "author": "Efim Belyakov (belyakov.efim@gmail.com)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn run migration:up && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/dstu-helper/test/jest-e2e.json",
    "migration:up": "yarn mikro-orm migration:up",
    "migration:down": "yarn mikro-orm migration:down"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.2",
    "@mikro-orm/cli": "^5.4.2",
    "@mikro-orm/core": "^5.4.0",
    "@mikro-orm/migrations": "^5.4.2",
    "@mikro-orm/nestjs": "^5.1.1",
    "@mikro-orm/postgresql": "^5.4.0",
    "@mikro-orm/reflection": "^5.4.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/glob": "^8.0.0",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^29.2.4",
    "amqp-connection-manager": "^4.1.9",
    "amqplib": "^0.10.3",
    "bull": "^4.10.2",
    "ember-truth-helpers": "^3.1.1",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.2.3",
    "js-levenshtein": "^1.1.6",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.5",
    "moment": "^2.29.4",
    "natural": "^6.1.2",
    "node-telegram-bot-api": "^0.61.0",
    "node-vk-bot-api": "^3.5.0",
    "normalize-text": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "string": "^3.3.3",
    "typeorm": "^0.3.11",
    "vk-io": "^4.7.0",
    "wikijs": "^6.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.15",
    "@types/i18n": "^0.13.2",
    "@types/js-levenshtein": "^1.1.0",
    "@types/lodash": "^4.14.177",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.11.15",
    "@types/node-telegram-bot-api": "^0.57.6",
    "@types/string": "^0.0.31",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.1",
    "supertest": "^6.1.3",
    "ts-essentials": "^9.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.3.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/orm.config.js"
    ]
  }
}
