{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dstu-helper/src",
  "compilerOptions": {
    "assets": [
      {
        "include": "modules/**/*.hbs",
        "watchAssets": true
      }
    ],
    "webpack": false,
    "tsConfigPath": "apps/dstu-helper/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/dstu-helper",
  "projects": {
    "dstu-helper": {
      "type": "application",
      "root": "apps/dstu-helper",
      "entryFile": "main",
      "sourceRoot": "apps/dstu-helper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dstu-helper/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "bot": {
      "type": "library",
      "root": "libs/bot",
      "entryFile": "index",
      "sourceRoot": "libs/bot/src",
      "compilerOptions": {
        "tsConfigPath": "libs/bot/tsconfig.lib.json"
      }
    }
  }
}
